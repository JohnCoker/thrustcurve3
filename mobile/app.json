{
  "expo": {
    "name": "ThrustCurve",
    "slug": "tctg",
    "version": "2.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "tctg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.tctg"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-android-fg.png",
        "backgroundImage": "./assets/images/icon-android-bg.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.tctg"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
